<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd"
	version="6">

	<display-name>personaservice-app</display-name>

	<module>
		<ejb>personaservice-persistence-1.0.jar</ejb>
	</module>
	<module>
		<ejb>personaservice-services-1.0.jar</ejb>
	</module>
	<module>
		<web>
            <web-uri>personaservice-ws-1.0.war</web-uri>
            <context-root>web-service</context-root>
        </web>
	</module>
	<module>
		<web>
            <web-uri>personaservice-web-1.0.war</web-uri>
            <context-root>web-application</context-root>
        </web>
	</module>
	<library-directory>APP-INF/lib</library-directory>

</application>